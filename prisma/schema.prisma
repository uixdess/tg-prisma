// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        String @id
  firstname String
  ip        String
  amount    String
  username  String
  userid    String
  date      String
  url       String
  status    String
}
